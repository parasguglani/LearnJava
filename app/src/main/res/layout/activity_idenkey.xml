<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".idenkey">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ff252525"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView

                android:id="@+id/iden"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="150dp"
                android:layout_marginBottom="50dp"
                android:gravity="center_horizontal"
                android:text="Headings"
                android:textAlignment="center"
                android:textColor="#fff"
                android:textSize="25dp"
                android:textStyle="bold"
                tools:layout_conversion_absoluteHeight="34dp"
                tools:layout_conversion_absoluteWidth="107dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="0dp" />

            <TextView
                android:id="@+id/idendata"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginBottom="25dp"
                android:textColor="#fff"
                android:textSize="20dp"
                tools:layout_conversion_absoluteHeight="27dp"
                tools:layout_conversion_absoluteWidth="40dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="58dp" />

            <ListView
                android:id="@+id/idendata1"
                android:layout_width="wrap_content"
                android:layout_height="254dp"
                android:layout_marginLeft="30dp"
                android:background="@drawable/data"
                android:textColor="#fff"
                android:textSize="20dp"
                tools:layout_conversion_absoluteHeight="31dp"
                tools:layout_conversion_absoluteWidth="98dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="113dp" />

            <TextView
                android:id="@+id/key"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginBottom="25dp"
                android:textColor="#fff"
                android:textSize="22dp"
                android:textStyle="bold"
                tools:layout_conversion_absoluteHeight="30dp"
                tools:layout_conversion_absoluteWidth="94dp"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="180dp" />

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableRow>

                    <TextView
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:layout_height="match_parent"
                        android:background="@drawable/data"
                        android:padding="8dip"
                        android:text="abstract"
                        android:textColor="#fff"
                        android:textSize="18sp"
                        />

                    <TextView
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:background="@drawable/data"
                        android:padding="8dip"
                        android:text="assert"
                        android:textColor="#fff"
                        android:textSize="18sp" />

                    <TextView
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:background="@drawable/data"
                        android:padding="8dip"
                        android:text="boolean"
                        android:textColor="#fff"
                        android:textSize="18sp" />

                    <TextView
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:background="@drawable/data"
                        android:padding="8dip"
                        android:text="break"
                        android:textColor="#fff"
                        android:textSize="18sp" />
                </TableRow>

                <TableRow>

                    <TextView
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:background="@drawable/data"
                        android:padding="8dip"
                        android:text="byte"
                        android:textColor="#fff"
                        android:textSize="18sp" />

                    <TextView

                        android:background="@drawable/data"
                        android:gravity="right"
                        android:padding="8dip"
                        android:text="case"
                        android:textColor="#fff"
                        android:textSize="18sp" />

                    <TextView
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:background="@drawable/data"
                        android:padding="8dip"
                        android:text="catch"
                        android:textColor="#fff"
                        android:textSize="18sp" />

                    <TextView
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:background="@drawable/data"
                        android:padding="8dip"
                        android:text="char"
                        android:textColor="#fff"
                        android:textSize="18sp" />
                </TableRow>

                <TableRow>

                    <TextView
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:background="@drawable/data"
                        android:padding="8dip"
                        android:text="class"
                        android:textColor="#fff"
                        android:textSize="18sp" />

                    <TextView

                        android:background="@drawable/data"
                        android:gravity="right"
                        android:padding="8dip"
                        android:text="for"
                        android:textColor="#fff"
                        android:textSize="18sp" />

                    <TextView
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:background="@drawable/data"
                        android:padding="8dip"
                        android:text="goto"
                        android:textColor="#fff"
                        android:textSize="18sp" />

                    <TextView
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:background="@drawable/data"
                        android:padding="8dip"
                        android:text="while"
                        android:textColor="#fff"
                        android:textSize="18sp" />
                </TableRow>

                <TableRow>

                    <TextView
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:background="@drawable/data"
                        android:padding="8dip"
                        android:text="private"
                        android:textColor="#fff"
                        android:textSize="18sp" />

                    <TextView
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:background="@drawable/data"
                        android:padding="8dip"
                        android:text="protected"
                        android:textColor="#fff"
                        android:textSize="18sp" />

                    <TextView
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:background="@drawable/data"
                        android:padding="8dip"
                        android:text="static"
                        android:textColor="#fff"
                        android:textSize="18sp" />

                    <TextView
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:background="@drawable/data"
                        android:padding="8dip"
                        android:text="final"
                        android:textColor="#fff"
                        android:textSize="18sp" />
                </TableRow>
            </TableLayout>


        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>